@model IEnumerable<string>

<script type="text/javascript">
    loadFlightID();
</script>
<select id="dateDestination" class="form-control" onchange="loadFlightID()">
    @if (Model.Count() == 0)
    {
        <option disabled>Select departure time please</option>
    }
    else
    {
        foreach (var item in Model)
        {
            <option value = "@item" > @item </option >
        }
    }
</select>

