@model IEnumerable<string>

<script type="text/javascript">
    loadDateDeparture();
</script>
<select id="destinations" class="form-control" onchange="loadDateDeparture()">
    @if (Model.Count() == 0)
    {
        <option disabled>Select departure please</option>
    }
    else
    {
        foreach (var item in Model)
        {
            <option value = "@item" > @item </option >
        }
    }
</select>
